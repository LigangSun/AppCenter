<Application x:Class="SoonLearning.AppCenter.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Startup="Application_Startup" 
             StartupUri="MainWindow.xaml">
	
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Style/Data.xaml"/>
                <ResourceDictionary Source="Resources/Style/ColorTheme.xaml"/>
                <ResourceDictionary Source="Resources/Style/Icons.xaml"/>
                <ResourceDictionary Source="Resources/Style/Strings.xaml"/>
                <ResourceDictionary Source="Resources/Style/Button.xaml"/>
                <ResourceDictionary Source="Resources/Style/CheckBox.xaml"/>
                <ResourceDictionary Source="Resources/Style/Combobox.xaml"/>
                <ResourceDictionary Source="Resources/Style/Others.xaml"/>
                <ResourceDictionary Source="Resources/Style/ScollBar.xaml"/>
                <ResourceDictionary Source="Resources/Style/ListBox.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
