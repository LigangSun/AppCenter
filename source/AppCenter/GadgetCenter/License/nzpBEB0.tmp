<UserControl x:Class="SoonLearning.AppCenter.License.LicenseInputUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:lang="clr-namespace:SoonLearning.AppCenter.Resources"
             mc:Ignorable="d" 
             Background="{StaticResource BGMainBrush_Dark}"
             d:DesignHeight="380" d:DesignWidth="463">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="64"/>
            <RowDefinition />
            <RowDefinition Height="64"/>
        </Grid.RowDefinitions> 
        
        <TextBlock x:Name="titleTextBlock" />
        
        <TextBox x:Name="licenseTextBox" 
                 Grid.Row="1"
                 Margin="30, 10"/>
    </Grid>
</UserControl>
